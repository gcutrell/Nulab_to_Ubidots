<profile interval = "36000">
	<script name = "main">

		<device name="Phosphate" type="serial" port="/dev/ttyUSB1" baudrate="9600" parity="None" stopbits="1" databits="8" handshake="None" dtr="False" />
		<send string="/9M3{13}"  behavior="tt" var="NLreply" trigger="{13}" terminator="." keeptrigger="false" keepterminator="true" ms="0" />

		<log message = "[NLreply]" />
		
		</kill>
	</script>
</profile>
		
